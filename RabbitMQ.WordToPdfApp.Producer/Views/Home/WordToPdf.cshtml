@model WordToPdf
@{
    ViewData["Title"] = "WordToPdfPage";
}

<h1>WordToPdf Page</h1>
<h2>@ViewBag.result</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="WordToPdf" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="File" class="control-label"></label>
                <input asp-for="File" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input type="email" asp-for="Email" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Convert" class="btn btn-primary mt-3" />
            </div>

        </form>
    </div>
</div>

